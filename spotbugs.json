    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "url": "https://repo.maven.apache.org/maven2/com/github/spotbugs/spotbugs/4.2.1/spotbugs-4.2.1.zip",
