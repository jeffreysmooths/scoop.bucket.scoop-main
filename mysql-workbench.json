                "extract_dir": "MySQL Workbench $version CE (winx64)"
            }
        },
        "hash": {
            "mode": "extract",
            "url": "$url.md5",
            "type": "md5",
            "find": "([a-z0-9]{32})\\s+(?:$basename)"
        }
    },
    "bin": [
        "MySQLWorkbench.exe",
        "mysql.exe",
        "mysqldump.exe"
    ]
