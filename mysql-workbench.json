        "architecture": {
            "32bit": {
                "url": "http://mysql.mirror.ac.za/Downloads/MySQLGUITools/mysql-workbench-community-$version-win32-noinstall.zip",
                "extract_dir": "MySQL Workbench $version CE (win32)"
            },
